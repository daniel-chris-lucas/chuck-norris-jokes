{
    "name": "daniel-chris-lucas/chuck-norris-jokes",
    "description": "Create Chuck Norris jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lucas",
            "email": "daniel.chris.lucas@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "DanielChrisLucas\\ChuckNorrisJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanielChrisLucas\\ChuckNorrisJokes\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DanielChrisLucas\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "DanielChrisLucas\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}
